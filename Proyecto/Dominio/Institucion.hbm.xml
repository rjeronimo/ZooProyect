<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
                   assembly="Proyecto"
                   namespace="Proyecto.Dominio"
                   default-lazy="false">
  <!-- Nombre de la clase Base -->
  <class name="Institucions" table="institucion">
    <!-- name es el nombre del entity-->
    <id name="Codigo" column="codigo">
      <generator class="increment" />
    </id>
    <property name="RUC" column="RUC" />
    <property name="Nombre" column="Nombre" />
  </class>
</hibernate-mapping>